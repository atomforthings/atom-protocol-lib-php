    "keywords": ["atom", "atomforthings", "atom for things", "iot", "internet of things", "protocol"],
    "license": "MIT",
    "version": "0.0.1-alpha.2",
    "authors": [
        {
            "name": "neerajsohal",
            "email": "hello@neerajkumar.name"
        }
